msgid "Command Line Options"
msgstr "Command Line Options"

#. type: cindex
#: basics.texi:63
#, no-wrap
msgid "Octave command options"
msgstr "Octave command options"

#. type: cindex
#: basics.texi:64
#, no-wrap
msgid "command options"
msgstr "command options"

#. type: cindex
#: basics.texi:65
#, no-wrap
